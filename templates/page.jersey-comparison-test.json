    "section_rich_text_VxWf4z": {
      "type": "section-rich-text",
      "blocks": {
        "heading_YC6Hpy": {
          "type": "heading",
          "settings": {
            "title": "Layer Up",
            "heading_font_size": "heading-large",
            "padding_bottom": 16,
            "heading_tag": "automatic"
